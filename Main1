import java.util.Scanner;
public class Main1{    
public static void main(String args[]){    
    int i;
    int flag=0;
  
    System.out.println("Enter Number ");
    Scanner sc= new Scanner(System.in);
    int n = sc.nextInt();
 
 
    for(i=2;i<=n/2;i++){
        if(n%i==0){
            flag=1;
            break;
        }
    }
    if(flag==1||n<=0||n==1) {
        System.out.println(n+" is Not prime number");
    }
    else{
        System.out.println(n+" is prime number");
    }
}    
}
